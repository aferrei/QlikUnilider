AtivLogLegada:

load
	AtivLogFerramentaID,
    AtivLogID,
    AtivLogIDSepa,
    AtivLogSituacaoID,
    AtivLogSubTipoID,
    AtivLogTipoRessupID,
    CaixaFechada,
    CdBarraAtivLog,
    CdDoca,
    CdEmp,
    CdUsrCanc,
    CdUsrCorte,
    CdUsrGerador,
    CdUsrResp,
    CodigoLacre,
    Corte,
    DtCanc,
    DtFim,
    DtGeracao,
    DtInicio,
    ErroValidacaoGeracao,
    Falha,
    MomentoGeraItens,
    NumLock,
    NuPalete,
    NuPed,
    NuRom,
    Peso,
    Placa,
    QtdeImpressao,
    QtdeProdutos,
    QtdeVolumes,
    RessuprimentoColetaID,
    RowID,
    SepaVolPadrao,
    SeqVolume,
    SitCorte,
    TipoSeparacaoID,
    Volume,
    diferenca,
    DtFimdt,
    Chave,
    DtIniciodt,
    WmsCategGrupoID;

SQL SELECT AtivLogFerramentaID,
    AtivLogID,
    iif(DATEDIFF(DAY,DtInicio,DtFim)is null,0,DATEDIFF(DAY,DtInicio,DtFim)) as diferenca,
    AtivLogIDSepa,
    AtivLogSituacaoID,
    AtivLogSubTipoID,
    AtivLogTipoRessupID,
    CaixaFechada,
    CdBarraAtivLog,
    CdDoca,
    CdEmp,
    CdUsrCanc,
    CdUsrCorte,
    CdUsrGerador,
    CdUsrResp,
    CodigoLacre,
    Corte,
    DtCanc,
    DtFim,
    DtGeracao,
    DtInicio,
    CAST(DtInicio AS DATE)AS DtIniciodt,
    CAST(DtFim AS DATE)AS DtFimdt,
    ErroValidacaoGeracao,
    Falha,
    MomentoGeraItens,
    NumLock,
    NuPalete,
    NuPed,
    NuRom,
    Peso,
    Placa,
    QtdeImpressao,
    QtdeProdutos,
    QtdeVolumes,
    RessuprimentoColetaID,
    RowID,
    SepaVolPadrao,
    SeqVolume,
    SitCorte,
    TipoSeparacaoID,
    Volume,
    WmsCategGrupoID,
    cast(cast(DtInicio as date)as varchar) + cast(CdUsrResp as varchar) as Chave
FROM MOINHO.dbo.AtivLog
WHERE
cast(DtInicio AS date) < cast(getdate()as date);
STORE *  from AtivLogLegada into C:\QlikView\Producao\QVD\LOGISTICA\AtivLogLegada.QVD;
DROP Table AtivLogLegada;


