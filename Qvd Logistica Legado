SQL SELECT AtivLogID,
    AtivLogItemID,
    AtivLogTipoItemID,
    BipLote,
    CdBarra,
    CdEmpCarreg,
    CdEmpEndDest,
    CdEmpEndOrig,
    CdEmpVol,
