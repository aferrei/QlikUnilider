AtivLogAtual:

load
	AtivLogFerramentaID,
    AtivLogID,
    AtivLogIDSepa,
    AtivLogSituacaoID,
    AtivLogSubTipoID,
    AtivLogTipoRessupID,
    CaixaFechada,
    CdBarraAtivLog,
    CdDoca,
    CdEmp,
    CdUsrCanc,
    CdUsrCorte,
    CdUsrGerador,
    CdUsrResp,
    CodigoLacre,
    Corte,
    DtCanc,
    DtFim,
    DtGeracao,
    DtInicio,
    ErroValidacaoGeracao,
    Falha,
    MomentoGeraItens,
    NumLock,
    NuPalete,
    NuPed,
    NuRom,
    Peso,
    Placa,
    QtdeImpressao,
    QtdeProdutos,
    QtdeVolumes,
    RessuprimentoColetaID,
    RowID,
    SepaVolPadrao,
    SeqVolume,
    SitCorte,
    diferenca,
    TipoSeparacaoID,
    Volume,
    Chave,
    DtFimdt,
    DtIniciodt,
    WmsCategGrupoID;

SQL SELECT AtivLogFerramentaID,
    AtivLogID,
    AtivLogIDSepa,
    AtivLogSituacaoID,
    AtivLogSubTipoID,
    AtivLogTipoRessupID,
    CaixaFechada,
    iif(DATEDIFF(DAY,DtInicio,DtFim)is null,0,DATEDIFF(DAY,DtInicio,DtFim)) as diferenca,
    CdBarraAtivLog,
    CdDoca,
    CdEmp,
    CdUsrCanc,
    CdUsrCorte,
    CdUsrGerador,
    CdUsrResp,
    CodigoLacre,
    Corte,
    DtCanc,
    DtFim,
    DtGeracao,
    DtInicio,
    CAST(DtInicio AS DATE)AS DtIniciodt,
    CAST(DtFim AS DATE)AS DtFimdt,
    ErroValidacaoGeracao,
    Falha,
    MomentoGeraItens,
    NumLock,
    NuPalete,
    NuPed,
    NuRom,
    Peso,
    Placa,
    QtdeImpressao,
    QtdeProdutos,
    QtdeVolumes,
    RessuprimentoColetaID,
    RowID,
    SepaVolPadrao,
    SeqVolume,
    SitCorte,
    TipoSeparacaoID,
    Volume,
    cast(cast(DtInicio as date)as varchar) + cast(CdUsrResp as varchar) as Chave,
    WmsCategGrupoID
FROM MOINHO.dbo.AtivLog
WHERE
cast(DtInicio AS date) = cast(getdate()as date);
STORE *  from AtivLogAtual into C:\QlikView\Producao\QVD\LOGISTICA\AtivLogAtual.QVD;
DROP Table AtivLogAtual;

