Jornada:
LOAD
	CdUsrResp,
    DtFim,
    jornada,
    Chave,
    trabalhado;
SQL SELECT CdUsrResp,
    DtFim,
    jornada,
    trabalhado,
    cast(cast(DtFim as date)as varchar) + cast(CdUsrResp as varchar) as Chave
FROM MOINHO.dbo.alx_jornada_qlik;
STORE *  from Jornada into C:\QlikView\Producao\QVD\LOGISTICA\Jornada.QVD;
DROP Table Jornada;

