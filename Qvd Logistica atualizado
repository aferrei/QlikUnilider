SQL SELECT AtivLogID,
    AtivLogItemID,
    AtivLogTipoItemID,
    BipLote,
    CdBarra,
    CdEmpCarreg,
    CdEmpEndDest,
    CdEmpEndOrig,
    CdEmpVol,
    CdForn,
    CdLocalEndDest,
    CdLocalEndOrig,
    CdProd,
    Descricao,
    DtValidade,
    Lote,
    NfeChaveAcesso,
    NuNf,
    NuPedCarreg,
    NuPedVol,
    Ordem,
    Original,
    Qtde,
    QtdeConf,
    rowid,
    SeqLote,
    SeqNfReci,
    SeqWmsEndDest,
    SeqWmsEndOrig,
    Situacao,
    Unidade,
    WmsEstoqueConsolidID
FROM MOINHO.dbo.AtivLogItem
where
AtivLogID IN (SELECT	
	AtivLogID
	FROM
	dbo.AtivLog 
	WHERE
	cast( DtInicio AS date) = cast(getdate()as date)
) ;
STORE *  from AtivLogItemAtual into C:\QlikView\Producao\QVD\LOGISTICA\AtivLogItemAtual.QVD;
